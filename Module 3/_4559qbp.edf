BLOCK EA-SEP RADFRAC
    PARAM NSTAGE=15 ALGORITHM=STANDARD MAXOL=200 DAMPING=NONE
    COL-CONFIG CONDENSER=TOTAL
    FEEDS ORGANIC 8 ON-STAGE
    PRODUCTS EAPROD 15 L / RECYC 1 L
    P-SPEC 1 1.
    COL-SPECS MASS-D=18500. MOLE-RR=5.
TITLE
SENSITIVITY S-1
    DEFINE PURITY2 MASS-FLOW STREAM=RECYC SUBSTREAM=MIXED  &
        COMPONENT=ETH UOM="kg/hr"
    TABULATE 1 "PURITY2"
    VARY BLOCK-VAR BLOCK=EA-SEP VARIABLE=MOLE-RR  &
        SENTENCE=COL-SPECS
    RANGE OPT-LIST=RANGE LOWER="1" UPPER="5" NPOINT="150"
