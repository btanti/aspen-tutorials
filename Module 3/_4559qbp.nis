DESIGN-SPEC DS-1
    DEFINE PURITY MASS-FRAC STREAM=EAPROD SUBSTREAM=MIXED  &
        COMPONENT=EA
    SPEC "PURITY" TO ".98"
    TOL-SPEC ".0001"
    VARY MASS-FLOW STREAM=WATER SUBSTREAM=MIXED COMPONENT=WATER  &
        UOM="kg/min"
    LIMITS "200" "800"
