BLOCK AA-SEP RADFRAC
    PARAM NSTAGE=16 ALGORITHM=STANDARD MAXOL=200 DAMPING=NONE
    COL-CONFIG CONDENSER=TOTAL
    FEEDS EFFLUENT 12 ON-STAGE
    PRODUCTS TERN 1 L / AA-RECYC 16 L
    P-SPEC 1 1.
    COL-SPECS QN=0.3 MOLE-RR=2.
    SPEC 1 MOLE-FRAC 1E-05 COMPS=AA STREAMS=TERN  &
        SPEC-DESCRIP="Mole purity, 1E-05, PRODUCT"
    VARY 1 MOLE-RR 1.2 6.
SENSITIVITY S-1
    DEFINE D1 MOLE-FRAC STREAM=TERN SUBSTREAM=MIXED  &
        COMPONENT=AA
    TABULATE 1 "D1"
    VARY BLOCK-VAR BLOCK=AA-SEP VARIABLE=QN SENTENCE=COL-SPECS  &
        UOM="Gcal/hr"
    RANGE OPT-LIST=RANGE LOWER="0.3" UPPER="2" NPOINT="100"
